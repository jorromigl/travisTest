    <validationRules>
        <fullName>BloquearCambioCuenta_InteraccionCerrada</fullName>
        <active>true</active>
        <errorConditionFormula>AND(RecordType.DeveloperName=&quot;CAC_Interaccion&quot;,OR(ISPICKVAL(Status,&quot;No Resuelto&quot;),ISPICKVAL(Status,&quot;Resuelto&quot;)), ISCHANGED( AccountId ))</errorConditionFormula>
        <errorMessage>No se puede modificar el cliente asociado una vez la interacción está cerrada.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAC_Motivo_Estado_NoResuelto</fullName>
        <active>true</active>
        <description>Se requiere informar el campo &apos;Motivo de estado&apos; en el caso de que una subinteracción se cierre con estado &apos;No Resuelto&apos;.</description>
        <errorConditionFormula>AND(RecordType.DeveloperName=&quot;CAC_Operativa&quot;,ISPICKVAL(Status, &quot;No resuelto&quot;),(ISBLANK(TEXT(CAC_Motivo_de_estado__c ))))</errorConditionFormula>
        <errorMessage>El campo Motivo de estado debe estar informado.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CAC_No_cerrar_sin_tipificar</fullName>
        <active>true</active>
        <description>Requerir que se tipifique una subinteracción para poder cerrarse.</description>
        <errorConditionFormula>AND( OR(ISPICKVAL(Status,&quot;No Resuelto&quot;),ISPICKVAL(Status,&quot;Resuelto&quot;)), RecordType.DeveloperName=&quot;CAC_Operativa&quot;,OR(ISBLANK(TEXT( CAC_Producto__c)), ISBLANK(TEXT(CAC_Servicio__c)), AND(ISBLANK(TEXT( CAC_Subproducto__c )), ISPICKVAL(CAC_Producto__c, &quot;Otros&quot;) )))</errorConditionFormula>
        <errorMessage>Es necesario completar la tipificación</errorMessage>
    </validationRules>
</CustomObject>
